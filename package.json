{
  "name": "@hekaiyu/element",
  "version": "1.1.4",
  "description": "Vue3 components library",
  "type": "module",
  "author": "KaiYu He",
  "license": "MIT",
  "keywords": [
    "Component",
    "UI",
    "Vue3"
  ],
  "homepage": "https://github.com/hky301/h-element/",
  "repository": {
    "type": "git",
    "url": "https://github.com/hky301/h-element/"
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "dist/index.css"
  ],
  "main": "./dist/umd/h-element.umd.cjs",
  "module": "./dist/es/h-element.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/es/h-element.js",
      "require": "./dist/umd/h-element.umd.cjs",
      "types": "./dist/types/index.d.ts"
    },
    "./dist/": {
      "import": "./dist/",
      "require": "./dist/"
    }
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search",
        "search-insights"
      ]
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "npm run build-only && npm run move-style",
    "preview": "vite preview",
    "build-only": "run-p build-es build-umd",
    "build-umd": "vite build --config vite.umd.config.ts",
    "build-es": "vite build --config vite.es.config.ts",
    "move-style": "move-file dist/es/index.css dist/index.css",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "prepublishOnly": "npm run build",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.3.0",
    "@fortawesome/vue-fontawesome": "^3.0.3",
    "@popperjs/core": "^2.11.6",
    "@vitepress-demo-preview/component": "^2.3.2",
    "@vitepress-demo-preview/plugin": "^1.2.2",
    "axios": "^1.3.4",
    "lodash-es": "^4.17.21"
  },
  "peerDependencies": {
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@tsconfig/node18": "^18.2.2",
    "@types/node": "^18.18.5",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.4.0",
    "jsdom": "^21.1.0",
    "move-file-cli": "^3.0.0",
    "npm-run-all2": "^6.1.1",
    "postcss-color-mix": "^1.1.0",
    "postcss-each": "^1.1.0",
    "postcss-each-variables": "^0.3.0",
    "postcss-for": "^2.1.1",
    "postcss-nested": "^6.0.1",
    "typescript": "~5.2.0",
    "vite": "^4.4.11",
    "vite-plugin-dts": "^3.6.3",
    "vitepress": "1.0.0-rc.25",
    "vitest": "^0.34.6",
    "vue": "^3.3.4",
    "vue-tsc": "^1.8.19"
  }
}